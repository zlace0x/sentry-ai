{
  "name": "sentry-ai",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Wallet sentry AI",
  "imports": {
    "#root/*": "./build/src/*"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "bun --watch ./src/main.ts",
    "start": "prisma migrate deploy && bun ./src/main.ts",
    "postinstall": "prisma generate",
    "start:force": "bun ./src/main.ts",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@ai-sdk/mistral": "^0.0.20",
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/parse-mode": "^1.10.0",
    "@grammyjs/types": "3.8.0",
    "@hono/node-server": "1.12.2",
    "@prisma/client": "5.16.1",
    "@subsquid/evm-abi": "^0.3.0",
    "ai": "^3.2.16",
    "callback-data": "1.1.1",
    "grammy": "1.24.1",
    "grammy-guard": "0.5.0",
    "hono": "4.4.3",
    "iso-639-1": "3.1.2",
    "modelfusion": "^0.137.0",
    "pino": "9.1.0",
    "pino-pretty": "11.1.0",
    "prisma": "^5.16.1",
    "tsx": "4.11.2",
    "viem": "^2.17.2",
    "znv": "0.4.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.3",
    "@types/bun": "^1.1.6",
    "@types/node": "20.14.10",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "tsc-watch": "6.2.0",
    "typescript": "5.5.3"
  },
  "lint-staged": {
    "*.ts": "eslint"
  }
}
